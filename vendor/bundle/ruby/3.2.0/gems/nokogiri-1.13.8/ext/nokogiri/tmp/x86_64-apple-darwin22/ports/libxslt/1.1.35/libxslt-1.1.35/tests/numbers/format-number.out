    format-number(-0.5,'0.' ) = -1.
    format-number( 0.0,'0.' ) = 0.
    format-number( 0.5,'0.' ) = 1.
    format-number( 1.0,'0.' ) = 1.
  </point-test>
  <point-test>
    format-number(-1.0, '.#') = -1.0
    format-number(-0.5, '.#') = -.5
    format-number( 0.0, '.#') = .0
    format-number( 0.5, '.#') = .5
    format-number( 1.0, '.#') = 1.0
  </point-test>
  <point-test>
    format-number(-1.0, '.##') = -1.0
    format-number(-0.5, '.##') = -.5
    format-number( 0.0, '.##') = .0
    format-number( 0.5, '.##') = .5
    format-number( 1.0, '.##') = 1.0
  </point-test>
  <point-test>
