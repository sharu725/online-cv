		%userinput.role.attrib;
		%local.userinput.attrib;
>
<!ENTITY % local.abbrev.attrib "">
<!ENTITY % abbrev.role.attrib "%role.attrib;">
<!ELEMENT abbrev (%word.char.mix;)*>
