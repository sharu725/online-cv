<t:titlepage element="article" wrapper="div" class="titlepage">
  <t:titlepage-content side="recto">
    <title/>
    <subtitle/>
    <corpauthor/>
    <authorgroup/>
    <author/>
    <releaseinfo/>
    <copyright/>
    <legalnotice/>
    <pubdate/>
    <revision/>
    <revhistory/>
    <abstract/>
  </t:titlepage-content>

  <t:titlepage-content side="verso">
  </t:titlepage-content>

  <t:titlepage-separator>
    <hr/>
